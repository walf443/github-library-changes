{
    "name": "Github Library Changes",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "js": ["main.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "https://rubygems.org/",
        "https://search.cocoapods.org/"
    ]
}
